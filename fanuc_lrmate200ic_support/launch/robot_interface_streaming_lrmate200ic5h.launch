<!--
  Manipulator specific version of 'robot_interface_streaming.launch'.

  Defaults provided for LR Mate 200iC/5H:
   - J23_factor = 1
   - use_bswap = true
   - 5 joints

  Usage:
    robot_interface_streaming_lrmate200ic5h.launch robot_ip:=<value>
-->
<launch>
  <arg name="robot_ip" />
  <arg name="J23_factor" default="1" />
  <arg name="use_bswap" default="true" />

  <rosparam command="load" file="$(find fanuc_lrmate200ic_support)/config/joint_names_lrmate200ic5h.yaml" />

  <include file="$(find fanuc_driver)/launch/robot_interface_streaming.launch">
    <arg name="robot_ip"   value="$(arg robot_ip)" />
    <arg name="J23_factor" value="$(arg J23_factor)" />
    <arg name="use_bswap"  value="$(arg use_bswap)" />
  </include>
</launch>
